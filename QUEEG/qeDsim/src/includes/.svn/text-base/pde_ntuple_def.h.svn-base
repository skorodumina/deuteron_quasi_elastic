#ifndef PDE_NTUPLE_DEF_H_
#define PDE_NTUPLE_DEF_H_

struct pde_event_t{
  float runno;
  float init_en;
  float e_sector;
  float e_en;
  float e_theta;
  float e_phi;
  float e_mom;
  float alpha;
  float e_paddle;
  float qsq;
  float w;
  float p_en;  
  float p_theta_m; // _m denotes measured quantities
  float p_phi_m;
  float p_mom_m;
  float p_mass_m;
  float p_theta_e; // _e denotes expected-from-kinematics quantities
  float p_phi_e;
  float p_mom_e;
  float p_mass_e;
  float p_beta;
  float p_pt;    // measured proron momentum transverse 
                 //to electron scattering plane
  float p_calc_e;//kinematic value for electron p,assuming elastic scattering 
                 //at measured angle
  float p_calc_p; //kinematic value for proton p, assuming elastic scattering
                  //at measured proton angle
  float p_calc_ep; //electron momentum calculated from measured proton angle
  float theta_calc_p; //proton theta for elastic scattering at measured 
                      //electron angle
  float theta_calc_ep; //proton theta for elastic scattering at measured electron 
                       //momentum
  float qsq_p; //qsq as calculated from proton angle
  float vert_x;
  float vert_y;
  float vert_z;
  float proton_z;
  float scid_pred;
  float scid_pred_dc;
  float scid_pred_part0;
  float scid_found;
  float scid_found_ntrk;
  float delta_t;
  float delta_beta;
  float status;
  float edep;
  float event_number;
};

#endif
